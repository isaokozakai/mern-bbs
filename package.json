{
  "name": "mern-bbs",
  "version": "1.0.0",
  "description": "BBS Application built with the MERN stack",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Isao Kozakai",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.1.2",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.7",
    "node-sass": "^4.13.1",
    "redux-devtools-extension": "^2.13.8"
  },
  "devDependencies": {
    "nodemon": "^1.19.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isaokozakai/mern-bbs.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/isaokozakai/mern-bbs/issues"
  },
  "homepage": "https://github.com/isaokozakai/mern-bbs#readme"
}
